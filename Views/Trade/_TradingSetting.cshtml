@model TradeSettingRequest

<div class="flex flex-col sm:flex-row">
   @if (Model != null)
   {
      <div class="flex flex-col">
         <p>Name: @Model.Name</p>
         <p>Risk Reward: @Model.RiskReward</p>
         <p>Retry Attempt: @Model.RetryAttempt</p>
         <p>Risk reward: @Model.RiskReward</p>
      </div>
   }
   else
   {
      <button class="btn btn-primary">Add settings</button>
   }
   <p hx-get="/trade/test-request">@ViewBag.Exchange</p>
</div>